{
  "name": "jqueryuitreecontrol",
  "version": "3.0.0",
  "description": "jQuery plugin for jquery-ui compliant treeview.",
  "main": "dist/jquery.techbytarun.jqueryuitreecontrol.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "jquery-ui",
    "tree",
    "treeview",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run build:ts && npm run build:webpack",
    "build:ts": "tsc",
    "build:webpack": "webpack --config webpack.config.js",
    "build:webpack:dev": "webpack --config webpack.config.js --mode development",
    "clean": "rm -rf dist/*",
    "dev": "npm run build && npm run watch",
    "dev:webpack": "npm run build:webpack:dev && npm run watch:webpack",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "format": "prettier --write **/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "validate": "npm run lint && npm run test && npm run build"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.33",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-jquery": "^1.5.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^22.1.0",
    "nodemon": "^3.1.11",
    "prettier": "^2.8.8",
    "terser-webpack-plugin": "^5.3.16",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "uglify-js": "^3.17.4",
    "webpack": "^5.104.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "jquery-ui": "^1.13.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
